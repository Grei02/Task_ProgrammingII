<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.taskprogrammingii.controller.EditWindowController">
   <children>
      <VBox alignment="CENTER" layoutX="333.0" layoutY="34.0" prefHeight="200.0" prefWidth="100.0" spacing="15.0" stylesheets="@Style.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label styleClass="jfx-titulo-label" stylesheets="@Style.css" text="Mantemimiento de Cooperativa " />
            <MFXTextField fx:id="txtNameCoop" floatingText="Modificar nombre de la cooperativa " prefHeight="49.0" prefWidth="553.0" />
            <Label text="Modificar el logo de cooperativa " />
            <ImageView fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onImageClicked" pickOnBounds="true" preserveRatio="true" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <MFXButton text="Cancelar " />
                  <MFXButton fx:id="btnSave" onAction="#onActionBtnSave" styleClass="btnSave" stylesheets="@Style.css" text="Guardar " />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
